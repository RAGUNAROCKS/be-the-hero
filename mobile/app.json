{
  "expo": {
    "name": "Be The Hero",
    "slug": "bethehero",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "36.0.0",
    "platforms": [
      "android"
    ],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E02041"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rocketseat.bethehero",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E02041"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
